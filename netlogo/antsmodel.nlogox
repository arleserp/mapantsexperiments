to set-polygon [ num-points ]
  ; Define parameters for the circle
  let circle-radius 0.75   ; The distance from the center (0,0)


  ; Initialize a counter variable
  let i 0 ; This will serve as our loop index, starting from 0

  ; Use a 'while' loop to iterate a precise number of times
  ; The loop continues as long as 'i' is less than 'num-points'
  while [i < num-points] [
    ; --- MODIFIED LINE FOR ANGLE CALCULATION ---
    ; Calculate the current angle in DEGREES for this point
    ; 360 / num-points is the angle increment per point for a full circle in degrees
    let current-angle ( i * (360 / num-points) )

    ; Optional: Add a show command to debug the angle values
    ;show (word "Loop " i ": Angle = " current-angle)

    ; Calculate x and y coordinates using trigonometry
    ; 'cos' and 'sin' are assumed to take degrees
    let x-coord (circle-radius * cos current-angle)
    let y-coord (circle-radius * sin current-angle)

    ;show (word "x: " x-coord ", y: " y-coord) ; Showing coordinates as well for debugging

    ; Add the food point
    add-f-point x-coord y-coord 5

    ; Increment the counter for the next iteration
    set i (i + 1)
  ]
end

to set-circle
  let num-points 12       ; The number of food points around the circle
  set-polygon num-points
end

