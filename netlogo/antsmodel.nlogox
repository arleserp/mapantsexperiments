to-report average-arrival-time
  ; Count the number of turtles with the 'ants' breed.
  let n_ants count turtles with [breed = ants]

  ; Check if there are any ants present in the model.
  ifelse n_ants > 0 [
    ; If there are ants, extract their arrival times.
    ; 'arrival-times' is assumed to be a list (e.g., global or agent-owned)
    ; where each item is a list, and the actual arrival time is at index 1
    ; (the second element) of that inner list.
    let times map [arrival -> item 1 arrival] arrival-times

    ; *** ROBUSTNESS / CLARITY CHECK ***
    ; Even though NetLogo's `mean` primitive reports 0 for an empty list,
    ; this explicit check makes the desired behavior clear:
    ; if there are no recorded arrival times, the average is 0.
    ifelse empty? times [
      ; If the list of times is empty, report 0 as the average.
      report 0
    ] [
      ; If there are times in the list, calculate and report their mean.
      report mean times
    ]
  ][
    ; If there are no ants at all, report 0 for the average.
    report 0
  ]
end

to-report std-arrival-time
  let n_ants count turtles with [breed = ants]

  ifelse n_ants > 0 [
    let times map [arrival -> item 1 arrival] arrival-times

    ; *** NEW: Check the size of 'times' before calculating standard-deviation ***
    ifelse length times >= 2 [
      report standard-deviation times
    ] [
      ; If there are 0 or 1 times, standard deviation is undefined or trivially 0.
      ; Reporting 0 is a common and sensible default in such cases.
      report 0
    ]
  ][
    ; If no ants, report 0
    report 0
  ]
end

