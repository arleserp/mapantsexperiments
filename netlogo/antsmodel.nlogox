to show-patch-chemical [p]
  set plabel  [chemical] of p
  set plabel-color white
end

to go  ;; forever button
  if ticks = 0 [ set initial-green-patches sum [food] of patches with [pcolor = green]]

  if release-ants-round > 0 and ticks = release-ants-round [ add-ants ]
  set green-patches sum [food] of patches with [pcolor = green]
  ask ants
  [ if who >= ticks [ stop ] ;; delay initial departure
    ifelse color = cyan
    [ look-for-food  ]       ;; not carrying food? look for it
    [ return-to-nest ]       ;; carrying food? take it back to nest
    wiggle
    fd 1
    let current-chemical [chemical] of patch-here
    set max-smell current-chemical
    write-ant-path ;;write ant.csv
  ]

  ;ask foodys [ set chemical food-amount ] ;; to discuss

  ;if count foodys > 0 [
  ;  diffuse chemical diffusion-rate
  ;]
  ask patches with [food > 0][
   set chemical 100
  ]

  ifelse  show-values = True  [  mostrar-valores ][ ocultar-valores ]

  if block-difussion-round > 0 and ticks < block-difussion-round [ ;; block diff
    ask patches [ ;; perform difussion and evaporation

      ifelse any? neighbors [ ;; difussion
        let promedio-vecinos (sum [chemical] of neighbors / count neighbors) ;round (sum [chemical] of neighbors / count neighbors)
        set chemical chemical + diffusion-rate * ( promedio-vecinos - chemical )
      ][
        ; Aquí se aplican las condiciones de frontera
        ; Por ejemplo, para paredes absorbentes:
        set chemical 0
      ]
        ;[
      recolor-patch
      ;set green-patches 0
      ;;set chemical chemical - 0.09 * (green-patches / initial-green-patches)
      set chemical chemical * (1 - evaporation-rate) ;; slowly evaporate chemical
    ]
  ]


  tick
end



to return-to-nest  ;; turtle procedure
  ifelse nest?
  [ ;; drop food and head out again
    set color cyan
    set max-smell 0
    set last-arrival-time ticks ;;last arrival time is when ant go to nest
    rt 180 ]
  [ ;set chemical chemical + ant-difussion-rate  ;; drop some chemical
    ifelse top-4-enabled [ uphill-nest-scent-random-top4 ] [ uphill-nest-scent ]
  ]     ;; head toward the greatest value of nest-scent
end

to look-for-food  ;; turtle procedure
  ;; current patch has food
  if food > 0
  [
    set ant-difussion-rate chemical
    set color blue      ;; pick up food
    if  eat-food = True  [
      set food food - 1        ;; and reduce the food source
    ]
    rt 180                   ;; and turn around
    let diff-ticks ticks - last-arrival-time
    set arrival-times lput (list who diff-ticks  pxcor pycor) arrival-times ;;add ants and tick of arrival
    stop
  ]

  ;;  Validate if there is neighboorhood with food
  let nearby-food one-of neighbors with [food > 0]
  if nearby-food != nobody [
    face nearby-food
    fd 1
    set ant-difussion-rate chemical
    set color blue      ;; pick up food
    if  eat-food = True  [
      set food food - 1        ;; and reduce the food source
    ]
    rt 180                   ;; and turn around
    let diff-ticks ticks - last-arrival-time
    set arrival-times lput (list who diff-ticks  pxcor pycor) arrival-times ;;add ants and tick of arrival
    stop
  ]


