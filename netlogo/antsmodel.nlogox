;;; sniff left and right, and go where the strongest smell is
;to uphill-nest-scent  ;; turtle procedure
;  let scent-ahead nest-scent-at-angle   0
;  let scent-right nest-scent-at-angle  45
;  let scent-left  nest-scent-at-angle -45
;  if (scent-right > scent-ahead) or (scent-left > scent-ahead)
;  [ ifelse scent-right > scent-left
;    [ rt 45 ]
;    [ lt 45 ] ]
;end
to uphill-nest-scent ;; turtle procedure
  let scent-ahead nest-scent-at-angle 0
  let scent-right45 nest-scent-at-angle 45
  let scent-right90 nest-scent-at-angle 90
  let scent-right135 nest-scent-at-angle 135
  let scent-left45 nest-scent-at-angle -45
  let scent-left90 nest-scent-at-angle -90
  let scent-left135 nest-scent-at-angle -135

  let strongest-scent scent-ahead
  let strongest-direction 0

  if scent-right45 > strongest-scent [
    set strongest-scent scent-right45
    set strongest-direction 45
  ]
  if scent-right90 > strongest-scent [
    set strongest-scent scent-right90
    set strongest-direction 90
  ]
  if scent-right135 > strongest-scent [
    set strongest-scent scent-right135
    set strongest-direction 135
  ]
  if scent-left45 > strongest-scent [
    set strongest-scent scent-left45
    set strongest-direction -45
  ]
  if scent-left90 > strongest-scent [
    set strongest-scent scent-left90
    set strongest-direction -90
  ]
  if scent-left135 > strongest-scent [
    set strongest-scent scent-left135
    set strongest-direction -135
  ]

  if strongest-direction != 0 [
    rt strongest-direction
  ]
end


to uphill-nest-scent-random-top4 ;; turtle procedure
  let scent-ahead nest-scent-at-angle 0
  let scent-right45 nest-scent-at-angle 45
  let scent-right90 nest-scent-at-angle 90
  let scent-right135 nest-scent-at-angle 135
  let scent-left45 nest-scent-at-angle -45
  let scent-left90 nest-scent-at-angle -90
  let scent-left135 nest-scent-at-angle -135

  let scent-list []

  set scent-list (list scent-ahead scent-right45 scent-right90 scent-right135 scent-left45 scent-left90 scent-left135)
  let direction-list [0 45 90 135 -45 -90 -135]

  let scent-direction-pairs (map [[a b] -> (list a b)] scent-list direction-list)

  let sorted-scent-directions sort-by [ [a b] -> first a > first b ]  scent-direction-pairs
  let top-4-scent-directions sublist sorted-scent-directions 1 4


  let dir one-of top-4-scent-directions
  rt item 1 dir
end

to wiggle  ;; turtle procedure
  rt random wiggle-angle
  lt random wiggle-angle
  if not can-move? 1 [ rt 180 ]
end

to-report nest-scent-at-angle [angle]
  let p patch-right-and-ahead angle 1
  if p = nobody [ report 0 ]
  report [nest-scent] of p
end

to-report chemical-scent-at-angle [angle]
  let p patch-right-and-ahead angle 1
  if p = nobody [ report 0 ]
  report [chemical] of p
end


to add-f-point [ pos_x pos_y f-size ] ;;
  ask patches [
    set food-point-id food-point-id + 1
    if (distancexy (pos_x * max-pxcor) (pos_y * max-pycor)) < f-size
    [
      set food-source-number food-point-id
      set food 1 ;one-of [ 1 ]
    ]

    recolor-patch
  ]
end

to set-one-point-north
