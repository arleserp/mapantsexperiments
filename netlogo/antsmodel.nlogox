to-report get-orthogonal-coordinates [x1 y1 x2 y2 dist]
  let d_x (x2 - x1)
  let d_y (y2 - y1)
  let vec_length sqrt (d_x ^ 2 + d_y ^ 2)

  if vec_length = 0 [
    user-message "Los puntos no pueden ser iguales; el vector dirección no tiene vec_length."
    report []  ; Salida vacía si los puntos son iguales
  ]

  ; Vectores ortogonales unitarios
  let ox-izq (- d_y / vec_length)
  let oy-izq (d_x / vec_length)

  let ox-der (d_y / vec_length)
  let oy-der (- d_x / vec_length)

  ; Redondear y crear las coordenadas como listas
  let coords-left (list (round (x2 + ox-izq * dist)) (round (y2 + oy-izq * dist)))
  let coords-right  (list (round (x2 + ox-der * dist)) (round (y2 + oy-der * dist)))

  report (list coords-left coords-right)
end


to uphill-chemical-new

  if debug [
    file-open (word movement-alg "uphill-debug.txt") ;DEBUG
  ]

  let nearby-patches neighbors ; get neigbours

  if any? nearby-patches [
    ;;DEBUG
    let current-chemical [chemical] of patch-here
    let here_x [pxcor] of  patch-here
    let here_y [pycor] of  patch-here

    if debug [
      ask patch here_x here_y [
        set pcolor yellow
      ]
    ]

    if debug [ ;; write current patch and neighbourhood info to file
      file-print (word "Current Patch: " pxcor "," pycor " Chemical: " current-chemical) ; Print info about the current patch
      file-print "Neighbors:"

      ; Create a list to store neighbor information
      let neighbor-info []
      ask nearby-patches [
        set neighbor-info lput (word pxcor "," pycor "," [chemical] of self) neighbor-info
        set pcolor red
      ]

      ; Now, in the observer context, print the collected information
      foreach neighbor-info [ info ->
        file-print (word "  " info)
      ]
    ]

    let M max-one-of nearby-patches [chemical] ;; 1. from the current position S: find the neighbour with maximum pheromone M (out of the 9 neighbours at 1 patch distance)
    let M_x [pxcor] of M
    let M_y [pycor] of M

    if debug [
      file-print (word "Max Patch: " M_x "," M_y " Chemical: " [chemical] of M )
      ask patch M_x M_y
      [
        set pcolor white
      ]
    ]

    let Ant_x [pxcor] of patch-here
    let Ant_y [pycor] of patch-here


    let ortogonales get-orthogonal-coordinates Ant_x Ant_y M_x M_y 1
    let OM-left item 0 ortogonales
    let OM-right item 1 ortogonales

    let OM-left_x item 0 OM-left
    let OM-left_y item 1 OM-left

    let OM-right_x item 0 OM-right
    let OM-right_y item 1 OM-right

    if (OM-left_x >= min-pxcor and OM-left_x <= max-pxcor and
      OM-left_y >= min-pycor and  OM-left_y <= max-pycor and OM-right_x >= min-pxcor and OM-right_x <= max-pxcor and
      OM-right_y >= min-pycor and  OM-right_y <= max-pycor ) [

      let patch-OM-left patch OM-left_x OM-left_y
      let patch-OM-right patch OM-right_x OM-right_y
      let ch-max-patch [chemical] of M
      let ch-OM-left [chemical] of  patch-OM-left
      let ch-OM-right [chemical] of patch-OM-right

      if debug [
        file-print(word "ch-max-patch" ch-max-patch)
        file-print(word "ch-OM-left" ch-OM-left)
        file-print(word "ch-OM-right" ch-OM-left)
        file-print (word "OM-left Patch: " OM-left_x "," OM-left_y " Chemical: " [chemical] of patch-OM-left )
        file-print (word "OM-right Patch: " OM-right_x "," OM-right_y " Chemical: " [chemical] of patch-OM-right)
        ask patch-OM-left
        [
          set pcolor violet
        ]
        ask patch-OM-right
        [
          set pcolor violet
        ]
      ]

      (ifelse
        ch-max-patch >= ch-OM-left and ch-max-patch >= ch-OM-right [  set max-smell ch-max-patch if debug [ face M file-print(word "choice M:" M) ]]
        ch-OM-left >= ch-OM-right and ch-OM-left >= ch-max-patch [  set max-smell ch-OM-left  face patch-OM-left if debug [file-print(word "choice OM-left:" patch-OM-left) ]]
        ch-OM-right >= ch-OM-left and ch-OM-right >= ch-max-patch [ set max-smell ch-OM-right face patch-OM-right if debug [file-print(word "choice OM-right:" patch-OM-right) ]]
        [ set max-smell ch-max-patch face M if debug[file-print(word "default choice M:" M) ]]
      )
;      if debug [
;        print-debug-info
;      ]
      forward 1
    ]
    file-close
  ]
  file-close
end



to print-debug-info
  file-open (word movement-alg "uphill-debug.txt")
  let current-agent self
  let current-patch patch-here
  let current-chemical [chemical] of current-patch

  let nearby-patches patches in-radius 1
  let surrounding-patches nearby-patches with [self != current-patch]
  let surrounding-info ""
  ask surrounding-patches [
    set surrounding-info (word surrounding-info "(" pxcor "-" pycor "-" [chemical] of self ") ")
  ]

  file-close
end

to uphill-chemical-radius

  let current-chemical [chemical] of patch-here
    let here_x [pxcor] of  patch-here
    let here_y [pycor] of  patch-here
      if debug [
      ask patch here_x here_y [
        set pcolor yellow
      ]
    ]


  let nearby-patches patches in-radius 2 ;;identify all patches within radius 2 (24 surrounding cells)
  let surrounding-patches nearby-patches with [self != [patch-here] of myself] ;; exclude current patch



   if debug [ ;; write current patch and neighbourhood info to file
      ;file-print (word "Current Patch: " pxcor "," pycor " Chemical: " current-chemical) ; Print info about the current patch
      ;file-print "Neighbors:"

      ; Create a list to store neighbor information
      let neighbor-info []
      ask nearby-patches [
        set neighbor-info lput (word pxcor "," pycor "," [chemical] of self) neighbor-info
        set pcolor red
      ]

      ; Now, in the observer context, print the collected information
      ;foreach neighbor-info [ info ->
       ; file-print (word "  " info)
      ;]
    ]

  if any? surrounding-patches [
    let max-chemical-patch max-one-of surrounding-patches [chemical]
    let max-chemical-value [chemical] of max-chemical-patch

    ;; If the maximum value is greater than what the and has already smelled, update and move towards it
    if max-chemical-value > max-smell [
      set max-smell max-chemical-value
      face max-chemical-patch
      if debug [
        let M max-chemical-patch;; 1. from the current position S: find the neighbour with maximum pheromone M (out of the 9 neighbours at 1 patch distance)
        let M_x [pxcor] of M
        let M_y [pycor] of M
        ;file-print (word "Max Patch: " M_x "," M_y " Chemical: " [chemical] of M )
        ask patch M_x M_y
        [
          set pcolor white
        ]
      ]
      forward 1
    ]

  ]

end


